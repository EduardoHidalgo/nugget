{
  "name": "nugget-ui",
  "version": "0.1.0",
  "description": "Librería de componentes React basados en Material-UI, enfocado en la fácil implementación y maquetación de layouts complejos.",
  "license": "MIT License",
  "contributors": [
    "Eduardo Hidalgo Díaz Rugama",
    "Diego Lovera Viga"
  ],
  "homepage": "https://github.com/boneless-chicken/Nugget-UI",
  "repository": {
    "type": "git",
    "url": "https://github.com/boneless-chicken/Nugget-UI"
  },
  "bugs": {
    "url": "https://github.com/boneless-chicken/Nugget-UI/issues",
    "email": "bonelesschicken.soft@gmail.com"
  },
  "files": [
    "build/*"
  ],
  "main": "src/index.js",
  "scripts": {
    "watch": "nodemon -w src --exec npm run build",
    "test": "react-scripts test",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "build:prod": "react-scripts build && cd build && ls | grep -v static | xargs rm -rf",
    "lint-init": "./node_modules/.bin/eslint --init",
    "lint": "./node_modules/.bin/eslint --config ./.eslintrc.json src/**",
    "lint-fix": "./node_modules/.bin/eslint --config ./.eslintrc.json src/** --fix",
    "analyze": "source-map-explorer 'build/static/js/*.js'"
  },
  "dependencies": {
    "create-react-app": "^3.1.1",
    "react-scripts": "3.1.x"
  },
  "devDependencies": {
    "@material-ui/core": "4.3.x",
    "@material-ui/icons": "4.2.x",
    "@material-ui/styles": "4.3.x",
    "eslint": "^6.1.0",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^1.7.0",
    "nodemon": "^1.19.1",
    "react": "16.9.x",
    "react-dom": "16.9.x",
    "source-map-explorer": "^2.0.1"
  },
  "peerDependencies": {
    "@material-ui/core": "4.3.x",
    "@material-ui/icons": "4.2.x",
    "@material-ui/styles": "4.3.x",
    "react": "16.9.x",
    "react-dom": "16.9.x"
  },
  "engines": {
    "node": "12.7.0",
    "npm": "6.10.0"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
